    "text = open(\"input.txt\").read().split(\",\")\n",
    "sum = 0\n",
    "for step in text:\n",
    "    c_val = 0\n",
    "    for char in step:\n",
    "        c_val += ord(char)\n",
    "        c_val *= 17\n",
    "        c_val = c_val % 256\n",
    "    sum += c_val\n",
    "print(sum)"
   ]
